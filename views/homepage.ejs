<h1>Accueil</h1>
<p>Page d'accueil du site internet</p>
    <p><%= req.session.test1%> <%= req.session.test2%></p>
    <% if (!req.session.authenticated) {
        if (req.session.tried == true) { %>
        <p>Authentication failed. Is your login or password incorrect ?</p>
      <% } %>
      <form method="post" action="/login">
        <input type="text" name="login" placeholder="Login"/>
        <input type="password" name="password" placeholder="Password"/>
        <input type="submit" value="submit" />
      </form>
    <p><%= req.session.authenticated %> <%= req.session.login %> <%= req.session.tried %></p>
    <% } else { %>
      <p>Bonjour <%= req.session.firstName%> !</p>

    <% }%>
